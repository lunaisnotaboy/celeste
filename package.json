{
  "name": "celeste",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "packageManager": "yarn@3.6.0",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "lint": "eslint --ext \"ts,tsx\" --max-warnings 0 --report-unused-disable-directives \"./src\"",
    "lint:fix": "eslint --ext \"ts,tsx\" --fix --max-warnings 0 --report-unused-disable-directives \"./src\"",
    "lint:prettier": "prettier --check \"./src\"",
    "lint:prettier:fix": "prettier --write \"./src\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.40.1",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "prettier": "^2.8.8",
    "typescript": "5.0.4",
    "vite": "^4.3.9"
  }
}
